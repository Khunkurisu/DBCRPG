window "mainWindow"
	elem "mainWindow"
		type = MAIN
		pos = 555,0
		size = 1280x670
		anchor1 = none
		anchor2 = none
		is-default = true
		saved-params = "pos;size;is-minimized;is-maximized"
		title = "Test Window"
		macro = "macro"
	elem "mainvsplit"
		type = CHILD
		pos = 0,0
		size = 1280x686
		anchor1 = 0,0
		anchor2 = 100,100
		background-color = #d26900
		saved-params = "splitter"
		left = "mapWindow"
		right = "secondaryvsplit"
		is-vert = true
		splitter = 68

window "mapWindow"
	elem "mapWindow"
		type = MAIN
		pos = 555,0
		size = 640x480
		anchor1 = none
		anchor2 = none
		background-color = #000000
		saved-params = "pos;size;is-minimized;is-maximized"
		titlebar = false
		statusbar = false
		can-close = false
		can-minimize = false
		can-resize = false
		is-pane = true
	elem "map1"
		type = MAP
		pos = 0,0
		size = 640x499
		anchor1 = 0,0
		anchor2 = 100,100
		is-default = true
		saved-params = "icon-size"

window "chatWindow"
	elem "chatWindow"
		type = MAIN
		pos = 555,0
		size = 339x440
		anchor1 = none
		anchor2 = none
