		can-resize = false
		is-pane = true
	elem "emoteButton"
		type = BUTTON
		pos = 275,413
		size = 61x27
		anchor1 = 81,94
		anchor2 = 99,100
		background-color = none
		saved-params = "is-checked"
		text = "Emote"
		command = "Emote"
	elem "oocToggle"
		type = BUTTON
		pos = 212,413
		size = 60x27
		anchor1 = 63,94
		anchor2 = 80,100
		background-color = none
		saved-params = "is-checked"
		text = "OOC Toggle"
		command = ".winset \"oocToggle.is-checked=true ? testInput.command= OOC : testInput.command= Say\""
		button-type = pushbox
	elem "testOutput"
		type = OUTPUT
		pos = 0,0
		size = 339x408
		anchor1 = 0,0
		anchor2 = 100,93
		text-color = #4e4e4e
		background-color = #d26900
		is-default = true
		saved-params = "max-lines"
	elem "testInput"
		type = INPUT
		pos = 0,413
		size = 210x27
		anchor1 = 0,94
		anchor2 = 62,100
		text-color = #ffffff
		background-color = #000000
		saved-params = ""
		command = "Say \""

